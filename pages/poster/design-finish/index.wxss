.container{padding:20px 30px !important}
.header{display:flex;justify-content:center;align-items:center;margin-bottom:20px;font-weight:600;font-size:16px;height:26px}
.header .iconfont{color:#07c160;margin-right:10px;font-size:24px}
.body .wrapper{background-color:#e3e6e9;padding:20px 50px;height:50vh;border-radius:10px}
.body .wrapper .van-image{width:100%;height:100%}
.body .wrapper .van-image .van-image__img{margin:0 auto}
.body .wrapper .van-image__loading{background-color:#e3e6e9}
.body .tips{margin:10px 0}
.body .tips .van-divider{margin-left:30px;margin-right:30px}
.body .op{display:flex;justify-content:space-between}
.body .op .van-button{width:150px}