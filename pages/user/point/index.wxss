.container{padding:0 !important}
.header{display:flex;justify-content:center;align-items:center;height:100px;color:#fff;background:#ff9f7c url(data:image/png;base64,) no-repeat;background-size:100%}
.header .point{font-size:36px;font-weight:600}
.header .tips{font-size:12px;margin-left:5px;font-weight:normal}
.body{padding:15px}
.body .do{display:flex;justify-content:space-between;padding:10px 0;border-bottom:1px solid #ebedf0}
.body .do .title{font-size:20px;font-weight:500}
.body .do .more{display:flex;align-items:center}
.body .do .more .iconfont{margin-left:5px;font-size:14px}
.body .task{margin-top:10px}
.body .task .item{display:flex;justify-content:space-between;align-items:center;padding:20px 0;border-bottom:1px solid #ebedf0}
.body .task .item .left{display:flex;height:44px}
.body .task .item .iconfont{font-size:40px;width:40px;margin-right:10px}
.body .task .item .iconfont.icon-calendar-check-fill{color:#faa651}
.body .task .item .iconfont.icon-wechat-fill{color:#39b54a}
.body .task .item .iconfont.icon-robot-fill{color:#2c83ff}
.body .task .item .setting .top{display:flex;margin-bottom:5px}
.body .task .item .setting .top .title{font-size:16px;font-weight:500}
.body .task .item .setting .top .point{margin-left:5px;color:#ff9f7c}
.body .task .item .setting .bottom{font-size:12px;color:#7d7e80}
.body .task .item .van-button{width:100px;background-color:#ff9f7c !important;border-color:#ff9f7c !important}
.popup-signin{width:70%}
.popup-signin.van-popup--center.van-popup--round{border-radius:10px}
.popup-signin .popup{padding:20px 30px;text-align:center}
.popup-signin .popup image{margin-bottom:20px;width:100px;height:100px}
.popup-signin .popup .title{font-size:20px;font-weight:500;margin-bottom:20px}
.popup-signin .popup .point{font-size:18px;color:#ff9f7c;margin-bottom:20px}
.popup-signin .popup .point .text{font-size:14px;margin-left:2px}
.popup-signin .popup .van-button{width:200px;background-color:#ff9f7c !important;border-color:#ff9f7c !important}